{
  "schemaVersion": 1,
  "id": "enhanced",
  "version": "${version}",
  "name": "Enhanced",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "Mohi0321"
  ],
  "contact": {
    "sources": "https://github.com/Mohi0321/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.example.addon.Enhanced"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": [
      "1.21.10"
    ],
    "meteor-client": "*"
  }
}
